<binding force-classes="true">
    <!--2008-02-01-->
    <mapping name="DescribeAvailabilityZonesResponse" class="edu.ucsb.eucalyptus.msgs.DescribeAvailabilityZonesResponseType" extends="edu.ucsb.eucalyptus.msgs.EucalyptusMessage">
        <structure map-as="edu.ucsb.eucalyptus.msgs.EucalyptusMessage"/>
        <collection name="availabilityZoneInfo" field="availabilityZoneInfo" usage="required" factory="org.jibx.runtime.Utility.arrayListFactory">
            <structure name="item" map-as="edu.ucsb.eucalyptus.msgs.ClusterInfoType"/>
        </collection>
    </mapping>
    <!--2008-02-01-->
    <mapping class="edu.ucsb.eucalyptus.msgs.ClusterInfoType" abstract="true">
        <value name="zoneName" field="zoneName"/>
        <value name="zoneState" field="zoneState"/>
    </mapping>
    <!--2008-02-01-->
    <mapping name="DescribeAvailabilityZones" class="edu.ucsb.eucalyptus.msgs.DescribeAvailabilityZonesType">
        <collection name="availabilityZoneSet" field="availabilityZoneSet" factory="org.jibx.runtime.Utility.arrayListFactory" item-type="java.lang.String">
            <structure name="item">
                <value name="zoneName"/>
            </structure>
        </collection>
    </mapping>
    <!--2008-12-01-->
    <mapping name="DescribeRegionsResponse" class="edu.ucsb.eucalyptus.msgs.DescribeRegionsResponseType" extends="edu.ucsb.eucalyptus.msgs.EucalyptusMessage">
        <structure map-as="edu.ucsb.eucalyptus.msgs.EucalyptusMessage"/>
        <collection name="regionInfo" field="regionInfo" usage="required" factory="org.jibx.runtime.Utility.arrayListFactory">
            <structure name="item" map-as="edu.ucsb.eucalyptus.msgs.RegionInfoType"/>
        </collection>
    </mapping>
    <!--2008-12-01-->
    <mapping class="edu.ucsb.eucalyptus.msgs.RegionInfoType" abstract="true">
        <value name="regionName" field="regionName"/>
        <value name="regionEndpoint" field="regionEndpoint"/>
    </mapping>
    <!--2008-12-01-->
    <mapping name="DescribeRegions" class="edu.ucsb.eucalyptus.msgs.DescribeRegionsType">
        <collection name="regionSet" field="regions" factory="org.jibx.runtime.Utility.arrayListFactory" item-type="java.lang.String">
            <structure name="item">
                <value name="regionName"/>
            </structure>
        </collection>
    </mapping>

</binding>